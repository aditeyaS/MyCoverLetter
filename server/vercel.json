{
  "version": 2,
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "buildCommand": "num run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "dist/index.js" }]
}
